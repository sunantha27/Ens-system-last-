<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="appewtc.masterung.enssystem">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HubActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SignUpActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".InformActivity"
            android:screenOrientation="portrait" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity
            android:name=".MapsActivity"
            android:label="@string/title_activity_maps"
            android:screenOrientation="portrait" />
        <activity
            android:name=".NewsListView"
            android:screenOrientation="portrait" />
        <activity
            android:name=".NewsDetail"
            android:screenOrientation="portrait" />
        <activity
            android:name=".CallListView"
            android:screenOrientation="portrait" />
        <activity
            android:name=".CallPhone"
            android:screenOrientation="portrait" />
        <activity
            android:name=".StaticActivity"
            android:screenOrientation="landscape" />
        <activity android:name=".ChooseTypeGraph" />
        android:name=".StaticDistrict"
        <activity android:name=".StaticDistrict" />
    </application>

</manifest>